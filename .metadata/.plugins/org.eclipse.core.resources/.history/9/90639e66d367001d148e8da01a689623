package conditionStatementsRevision;

public class SwitchCaseStatement {
	public static int getStudentMark() {
		int marks = 425;
		return marks;
	}

	public static void main(String[] args) {
//		print student name based on his roll no
		int rollNum = 7;
		switch (rollNum) {

		case 1:
			System.out.println("Ajay");
			break;
		case 2:
			int result = SwitchCaseStatement.getStudentMark();
			System.out.println("Bharat");
			System.out.println(result);
			break;
		case 3:
			System.out.println("Chetan");
			break;
		case 4:
			System.out.println("Dipawali");
			break;
		default:
			System.out.println("Enter valid Roll No");
			break;
		}
		System.out.println("Program ends here");

	}

}
